{
  "name": "text-escape",
  "version": "1.1.3",
  "private": true,
  "description": "一个简单易用的在线文本转义工具，支持换行符和双引号的转义与反转义",
  "author": "wxingheng",
  "scripts": {
    "dev": "next dev --turbopack -p 3004",
    "build": "next build",
    "start": "next start -p ${PORT:-3005}",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "version-auto": "jcommon-node -va -t",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop text-escape",
    "pm2:restart": "pm2 restart text-escape",
    "pm2:logs": "pm2 logs text-escape",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "next": "15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "jcommon-node": "^1.0.14",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "typescript": "^5"
  },
  "engines": {
    "node": "20"
  },
  "time": "250401202509"
}
